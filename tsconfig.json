{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "exclude": [
    "android",
    "ios",
    "dist"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "src/style/colors.js",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
